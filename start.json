{
  "$jason": {
    "head": {
      "title": "KOSMO",
      "actions": {
        "$load": {
          "type": "$set",
          "options": {
            "bg": "#9E9E9E"
          },
          "success": {
            "type": "$render"
          }
        }
      },
      "templates": {
        "body": {
          "header": {
            "style": {
              "background": "#FF9800",
              "color": "#212121"
            },
            "title": {
              "type": "image",
              "style": {
                "align": "left",
                "left": "20",
                "width": "120",
                "height": "50"
              },
              "url": "https://DinaDurykina.jasons.github.io/KOSMO_WORK/kosmo-kids_name.png"
            },
            "menu": {
              "text": "+7(499)397-70-77",
              "style": {
                "color": "#212121",
                "font": "Roboto",
                "size": "16"
              },
              "action": {
                "type": "$util.toast",
                "options": {
                  "text": " Сеть билингвальных\n детских садов\n и детских центров"
                }
              }
            }
          },
          "style": {
            "background": "{{$get.bg}}"
          },
          "layers": [
            {
              "type": "label",
              "text": " ЖИЗНЬ\n НАЧИНАЕТСЯ \n С НУЛЯ:))",
              "style": {
                "background": "rgba(0,0,0,7)",
                "color": "#ffffff",
                "width": "310",
                "padding": "10",
                "left": "10",
                "font": "Roboto",
                "size": "36",
                "corner_radius": "7",
                "bottom": "265"
              },
              "action": {
                "type": "$set",
                "options": {
                  "bg": {
                    "type": "camera"
                  }
                },
                "success": {
                  "type": "$render"
                }
              }
            },
            {
              "type": "button",
              "text": "Нажмите и мы Вам позвоним!",
              "style": {
                "background": "#FF4081",
                "color": "#ffffff",
                "width": "310",
                "height": "50",
                "padding": "10",
                "left": "10",
                "font": "Roboto",
                "size": "20",
                "corner_radius": "7",
                "align": "center",
                "bottom": "170"
              },
              "action": {
                "type": "$util.alert",
                "options": {
                  "title": "ЗАКАЗАТЬ ЗВОНОК:",
                  "description": "Представьтесь, \nи мы перезвоним Вам",
                  "form": [{
                    "name": "username",
                    "placeholder": "Введите имя"
                  }, {
                    "name": "number",
                    "placeholder": "+7"
                  }]
                }
              }
            },
            {
              "type": "button",
              "text": "Танцевальные направления",
              "style": {
                "background": "#C5CAE9",
                "color": "#212121",
                "width": "310",
                "height": "50",
                "padding": "10",
                "left": "10",
                "font": "Roboto",
                "size": "20",
                "corner_radius": "7",
                "align": "center",
                "bottom": "105"
              },
              "action": {
                "type": "$util.toast",
                "options": {
                  "text": "Нажали на кнопку направления"
                }
              }
            }
          ],
          "footer": {
            "tabs": {
              "style": {
                "background": "#FF9800",
                "color": "#000000"
              },
              "items": [
                {
                  "image": "https://DinaDurykina.github.io/icons8-vk.com-24.png",
                  "url": "https://DinaDurykina.github.io/7_br_vk.json"
                },
                {
                  "image": "https://DinaDurykina.github.io/icons8-home-page-24.png",
                  "url": "https://DinaDurykina.github.io/7_br_home.json"
                },
                {
                  "image": "https://DinaDurykina.github.io/icons8-marker-24.png",
                  "url": "https://DinaDurykina.github.io/7_br_map.json"
                }
              ]
            }
          }
        }
      }
    }
  }
}