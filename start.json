{
  "$jason": {
    "head": {
      "title": "Native",
      "actions": {
        "$load":    { "type": "$render" },
        "navigate": {"type": "$default" }
      },
      "templates": {
        "body": {
          "header": {
            "title": {
              "type": "image",
              "style": {
                "align": "left",
                "left": "20",
                "width": "12",
                "height": "12"
              },
              "url": "https://DinaDurykina.github.io/icons8-home-page-24.png"
            },
            "style": {
              "background": "#A9A9A9"
            },
            "menu": {
              "type": "image",
              "style": {
                "align": "right",
                "left": "20",
                "width": "12",
                "height": "12",
                "background": "#A9A9A9"
              },
              "url": "https://DinaDurykina.github.io/icons8-connect-24.png"
            },
            "action": {
              "type": "$util.toast",
              "options": {
                "text": "Считать путь страницы с контактами и поставить ссылку на него"
              }
            }
          }
        },
        "background": {
          "type": "html",
          "url": "http://www.brazuka.ru/",
          "action": { "trigger": "navigate" }
        },

        "footer": {
          "tabs": {
            "style": {
              "background": "#A9A9A9"	},
            "items": [{
              "image": "https://DinaDurykina.github.io/icons8-camera-24.png",
              "action": {
                "type": "$util.toast",
                "options": {
                  "text": "Включить камеру, а затем фото сложитьв галерею"
                }
              }
            }, {
              "image": "https://DinaDurykina.github.io/icons8-cut-paper-24.png",
              "action": {
                "type": "$util.toast",
                "options": {
                  "text": "Сделать скринэкрана и сложить в галерею"
                }
              }
            }, {
              "image": "https://DinaDurykina.github.io/icons8-microphone-24.png",
              "action": {
                "type": "$util.toast",
                "options": {
                  "text": "Включить микрофон, записать звук, сложить в папку аудио"
                }
              }
            }]
          }
        }
      }
    }
  }
}
}