{
  "$jason": {
    "head": {
      "title": "TemplateNativeNews",
      "actions": {
        "$load":    { "type": "$render" },
        "navigate": {"type": "$default" }
      },
      "templates": {
        "body": {
          "header": {
            "title": {
              "type": "image",
              "style": {
                "align": "left",
                "left": "20",
                "width": "12",
                "height": "12"
              },
              "url": "https://DinaDurykina.github.io/squared-menu.png"},
            "style": {"background": "#A9A9A9"},
            "menu": {
              "text": "Choose",
              "style": {
                "color": "#0000ff",
                "font": "HelveticaNeue-Bold",
                "size": "12"
              },
              "action": {
                "type": "$util.toast",
                "options": {
                  "text": "Good job!"
                }
              }
            }
          },
          "background": {
            "type": "html",
            "url": "http://www.brazuka.ru/",
            "action": { "trigger": "navigate" }
          },

          "footer": {
            "tabs": {
              "style": {
                "background": "#A9A9A9"	},
              "items": [{
                "image": "https://DinaDurykina.github.io/copy-2.png",
                "text": "Note",
                "style": {
                  "height": "12"
                },
                "url":"https://DinaDurykina.github.io/copy2teh.json"
              }, {
                "image": "https://DinaDurykina.github.io/translation.png",
                "text": "Translate",
                "style": {
                  "height": "12"
                },
                "url":"https://DinaDurykina.github.io/translate.json"
              }, {
                "image": "https://DinaDurykina.github.io/comments.png",
                "text": "Comment",
                "style": {
                  "height": "12"
                },
                "url":"https://DinaDurykina.github.io/commentteh.json"
              }]
            }
          }
        }
      }
    }
  }
}