{
  "$jason": {
    "head": {
      "title": "Calculator APP4WEB",
      "styles": {
        "bold": {
          "font": "HelveticaNeue-CondensedBold",
          "size": "15"
        },
        "padded": {
          "padding": "10"
        },
        "normal": {
          "font": "HelveticaNeue",
          "size": "12"
        }
      },
      "actions": {
        "$foreground": {
          "type": "$reload"
        }
      }
    },
    "body": {
